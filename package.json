{
  "name": "@frontztech/nextai-dev",
  "version": "0.2.5",
  "description": "NextAI Dev Framework - AI Development Workflow Orchestrator",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "nextai": "./bin/nextai.js"
  },
  "scripts": {
    "dev": "tsx watch src/cli/index.ts",
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "ai",
    "workflow",
    "orchestrator",
    "developer-tools",
    "claude",
    "opencode",
    "cli"
  ],
  "author": {
    "name": "John F",
    "email": "john@frontz.tech"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JohnFrontzos/nextAI-dev.git"
  },
  "bugs": {
    "url": "https://github.com/JohnFrontzos/nextAI-dev/issues"
  },
  "homepage": "https://github.com/JohnFrontzos/nextAI-dev#readme",
  "dependencies": {
    "@inquirer/prompts": "^7.2.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "glob": "^11.0.0",
    "ora": "^8.1.1",
    "uuid": "^11.0.3",
    "yaml": "^2.6.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.16.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "bin",
    "resources"
  ]
}
